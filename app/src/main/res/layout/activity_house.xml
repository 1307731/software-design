<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.software.design.realestateapp.HouseActivity">

    <TextView
        android:id="@+id/txtPlotSize"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/textView13"
        android:layout_alignBottom="@+id/textView13"
        android:layout_marginStart="12dp"
        android:layout_toEndOf="@+id/textView9"
        android:text="Plot Size"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/txtEvalasfda"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/textView28"
        android:layout_below="@+id/textView28"
        android:layout_marginTop="17dp"
        android:text="Evaluation Amount:"
        android:textSize="18sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="136dp"
        tools:layout_editor_absoluteY="414dp" />

    <TextView
        android:id="@+id/textView28"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/textView15"
        android:layout_below="@+id/textView15"
        android:layout_marginTop="19dp"
        android:text="Pool:"
        android:textSize="18sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="136dp"
        tools:layout_editor_absoluteY="414dp" />

    <TextView
        android:id="@+id/textView15"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Number of Garages: "
        android:textSize="18sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="213dp"
        android:layout_marginTop="24dp"
        android:layout_below="@+id/textView9"
        android:layout_alignStart="@+id/textView9" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Size of Plot:"
        android:textSize="18sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="26dp"
        tools:layout_editor_absoluteY="99dp"
        android:layout_marginTop="23dp"
        android:layout_below="@+id/textView6"
        android:layout_alignStart="@+id/textView6" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Suburb:"
        android:textSize="18sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteY="26dp"
        tools:layout_editor_absoluteX="34dp"
        android:layout_marginTop="32dp"
        android:layout_marginStart="29dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Address:"
        android:textSize="18sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteY="59dp"
        tools:layout_editor_absoluteX="34dp"
        android:layout_below="@+id/textView5"
        android:layout_alignStart="@+id/textView5"
        android:layout_marginTop="28dp" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Size of House:"
        android:textSize="18sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteY="139dp"
        tools:layout_editor_absoluteX="16dp"
        android:layout_marginTop="29dp"
        android:layout_below="@+id/textView13"
        android:layout_alignStart="@+id/textView13" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Number of Bathrooms:"
        android:textSize="18sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteY="271dp"
        tools:layout_editor_absoluteX="16dp"
        android:layout_marginTop="26dp"
        android:layout_below="@+id/textView10"
        android:layout_alignStart="@+id/textView10" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Number of Bedrooms:"
        android:textSize="18sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteY="181dp"
        tools:layout_editor_absoluteX="5dp"
        android:layout_below="@+id/textView8"
        android:layout_alignStart="@+id/textView8"
        android:layout_marginTop="31dp" />

    <TextView
        android:id="@+id/txtSubName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/textView5"
        android:text="Suburb Name"
        android:textSize="18sp"
        android:layout_alignParentEnd="true"
        android:layout_toEndOf="@+id/textView8" />

    <TextView
        android:id="@+id/txtHouseSize"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/textView8"
        android:layout_alignBottom="@+id/textView8"
        android:layout_alignStart="@+id/txtPlotSize"
        android:text="House Size"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/txtAddName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/textView13"
        android:layout_alignParentEnd="true"
        android:layout_alignStart="@+id/txtSubName"
        android:text="Address of house"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/txtNumBed"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/textView10"
        android:layout_alignStart="@+id/txtHouseSize"
        android:text="Num Beds"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/txtNumBath"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/textView15"
        android:layout_alignStart="@+id/txtNumBed"
        android:text="Num Baths"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/txtNumGar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/textView15"
        android:layout_alignBottom="@+id/textView15"
        android:layout_alignStart="@+id/txtNumBath"
        android:text="Num Garages"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/txtPool"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Yes/No"
        android:textSize="18sp"
        android:layout_alignBaseline="@+id/textView28"
        android:layout_alignBottom="@+id/textView28"
        android:layout_alignEnd="@+id/txtPlotSize" />

    <TextView
        android:id="@+id/txtEval"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Evaluation Amount"
        android:textSize="18sp"
        android:layout_alignBaseline="@+id/txtEvalasfda"
        android:layout_alignBottom="@+id/txtEvalasfda"
        android:layout_toEndOf="@+id/textView10" />

    <ToggleButton
        android:id="@+id/favourite_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/txtEval"
        android:layout_alignParentBottom="true"
        android:text="ToggleButton" />

    <TextView
        android:id="@+id/favouriteView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/favourite_button"
        android:layout_alignBottom="@+id/favourite_button"
        android:layout_alignStart="@+id/txtEvalasfda"
        android:text="Favourite:"
        android:textSize="18sp"
        android:textStyle="bold" />

</RelativeLayout>
